<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:xs="http://www.w3.org/2001/XMLSchema"
                  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
                  xmlns:svc="http://esp.t-mobile.com/service"
                  xmlns:mo="http://esp.t-mobile.com/mo"
                  targetNamespace="http://esp.t-mobile.com/service">
   <wsdl:documentation>
      <version id="1.3.0" releaseDate="2016-10-15"/>
   </wsdl:documentation>
   <wsdl:types>
      <xs:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                 xmlns:eo="http://esp.t-mobile.com/2012/eo"
                 xmlns="http://esp.t-mobile.com/2012/eo"
                 targetNamespace="http://esp.t-mobile.com/2012/eo"
                 elementFormDefault="qualified"
                 attributeFormDefault="unqualified"
                 version="1.3">
         <xsd:complexType name="Usage">
		          <xsd:sequence>
			            <xsd:element name="billTo" type="eo:SubscriptionProfile" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Reference to the subscription that generated this usage.
                               </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="description" type="eo:Description" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Description about Service Usage
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="usageLine" type="eo:UsageLine" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation> 
The Usage Line contains the elements that reflect the consumption of a service by the subscriber.
                               </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="note" type="eo:Note" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation> 
Note about Service Usage
                               </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="balanceSummaryGroup" type="eo:BalanceSummaryGroup" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>A logical grouping of balance information</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="specificationGroup" type="eo:SpecificationGroup" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>A specification group allows for including additional information related to usage, such as limits, balances and expiration dates.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
		          <xsd:attribute name="id" type="eo:Identifier">
			            <xsd:annotation>
				              <xsd:documentation>Uniquely identifies the instance of the usage object.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
		          <xsd:attribute name="actionCode">
			            <xsd:annotation>
				              <xsd:documentation>Indicates the type of action to be performed with this instance of the object.  Recognized values are found in <link href="UsageEnum/ActionCode"/>
				              </xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
	        </xsd:complexType>
         <xsd:complexType name="UsageLine">
		          <xsd:sequence>
			            <xsd:element name="description" type="eo:Description" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Description about Service Usage Line
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="unitPrice" type="eo:Amount" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Unit Price of Service Usage
                               </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="parentUsageLineId" type="eo:Identifier" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Allows us to associate the usage of a feature with the overall usage of the service.
                               </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="usageAllocation" type="eo:UsageAllocation" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Call detail supporting the usage line.
                               </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="product" type="eo:ProductBusinessObjectProfile" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Reference to product
                               </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:choice>
				              <xsd:annotation/>
				              <xsd:sequence>
					                <xsd:element name="amount" type="eo:Amount" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation> 
Service usage line amount
                                </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="discountAmount" type="eo:Amount" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation> 
Service usage line discount amount
                                </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
				              </xsd:sequence>
				              <xsd:sequence>
					                <xsd:element name="quantity" type="eo:Quantity" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation> 
Service usage line summary quantity
                                </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="discountQuantity" type="eo:Quantity" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation> 
Service usage line discount summary quantity
                                </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
				              </xsd:sequence>
			            </xsd:choice>
		          </xsd:sequence>
		          <xsd:attribute name="id" type="eo:Identifier"/>
	        </xsd:complexType>
         <xsd:complexType name="UsageAllocation">
		          <xsd:sequence>
			            <xsd:element name="usagePeriod" type="eo:TimePeriod" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The date and time when the usage occurred.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="discountAmount" type="eo:Amount" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Total discount amount calculated for this usage allocation, based on the unit price and total discounted quantity.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="totalAmount" type="eo:Amount" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Total amount calculated for this usage allocation, based on the unit price and total quantity.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="monetaryIndicator" type="xsd:boolean" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Classifies the usage as pertaining to a monetary resource, such as a refill coupon.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="callDirection" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Identifies the call as inbound or outbound.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="description" type="eo:Description" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Description of usage allocation.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="location" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>A description of the location for the usage allocation.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="phoneCommunication" type="eo:PhoneCommunication" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Identifies the phone with which the customer was communicating.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="productId" type="eo:Identifier" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Identifies the product that represents the service being used by the customer in this allocation.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="unitPrice" type="eo:Amount" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The price per quantity for the usage allocation.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="usageAllocationResource" type="eo:UsageAllocationResource" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Details of the quantity associated with this usage. Could be MB, message count, or number of ringtones, etc. </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="specificationGroup" type="eo:SpecificationGroup" minOccurs="0"
                            maxOccurs="unbounded"/>
		          </xsd:sequence>
		          <xsd:attribute name="id" type="eo:Identifier"/>
	        </xsd:complexType>
         <xsd:complexType name="UsageAllocationResource">
		          <xsd:sequence>
			            <xsd:element name="typeCode" type="xsd:token" minOccurs="0"/>
			            <xsd:element name="originalTotalQuantity" type="eo:Quantity" minOccurs="0"/>
			            <xsd:element name="priorAdjustmentQuantity" type="eo:Quantity" minOccurs="0"/>
			            <xsd:element name="priorDisputedQuantity" type="eo:Quantity" minOccurs="0"/>
			            <xsd:element name="priorSettlementQuantity" type="eo:Quantity" minOccurs="0"/>
			            <xsd:element name="totalAdjustmentQuantity" type="eo:Quantity" minOccurs="0"/>
			            <xsd:element name="description" type="eo:Description" minOccurs="0"/>
		          </xsd:sequence>
		          <xsd:attribute name="id" type="eo:Identifier"/>
	        </xsd:complexType>
         <xsd:complexType name="SubscriptionProfile">
		          <xsd:sequence>
			            <xsd:element name="accountNumber" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Customer account number that includes this subscription.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="MSISDN" type="eo:MSISDN" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Subscription number.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="key" type="eo:ObjectKey" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Alternate keys for systems other than the system of record.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="activationDate" type="xsd:date" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Date when subscription's initial activation took place.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="contractPeriod" type="eo:DatePeriod" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Duration of the subscription contract.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="subscriptionStatus" type="eo:Status" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Statuses of the subscription in their specified time periods or usage contexts. See valid values for subscription status in <link href="SubscriptionEnum/SubscriptionStatus"/>
					                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
		          <xsd:attribute name="id" type="eo:Identifier">
			            <xsd:annotation>
				              <xsd:documentation>Uniquely identifies the subscription.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
		          <xsd:attribute name="actionCode" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>Indicates the type of change that has been made to this version, as compared to the previous version.  Recognized values are found in <link href="SubscriptionEnum/ActionCode"/>
				              </xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
	        </xsd:complexType>
         <xsd:complexType name="BaseType">
		          <xsd:sequence>
			            <xsd:element name="id" type="xsd:string" minOccurs="0"/>
			            <xsd:element name="operation" minOccurs="0">
				              <xsd:simpleType>
					                <xsd:annotation>
						                  <xsd:documentation>Operation Performed , Like Insert,Update,Delete,Query</xsd:documentation>
					                </xsd:annotation>
					                <xsd:restriction base="xsd:normalizedString">
						                  <xsd:enumeration value="INSERT"/>
						                  <xsd:enumeration value="UPDATE"/>
						                  <xsd:enumeration value="DELETE"/>
						                  <xsd:enumeration value="QUERY"/>
					                </xsd:restriction>
				              </xsd:simpleType>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="Amount">
		          <xsd:annotation>
			            <xsd:documentation>A number of monetary units specified in a currency where the unit of the currency is explicit or implied</xsd:documentation>
		          </xsd:annotation>
		          <xsd:simpleContent>
			            <xsd:extension base="xsd:decimal">
				              <xsd:attribute name="currencyCode" type="xsd:normalizedString" use="optional"/>
			            </xsd:extension>
		          </xsd:simpleContent>
	        </xsd:complexType>
         <xsd:complexType name="BinaryObject">
		          <xsd:annotation>
			            <xsd:documentation>A set of finite-length sequences of binary octets</xsd:documentation>
		          </xsd:annotation>
		          <xsd:simpleContent>
			            <xsd:extension base="xsd:base64Binary">
				              <xsd:attribute name="mimeCode" type="xsd:normalizedString" use="optional"/>
				              <xsd:attribute name="characterSetCode" type="xsd:normalizedString" use="optional"/>
				              <xsd:attribute name="fileName" type="xsd:normalizedString"/>
			            </xsd:extension>
		          </xsd:simpleContent>
	        </xsd:complexType>
         <xsd:complexType name="TimePeriod">
		          <xsd:annotation>
			            <xsd:documentation>A span of time defined by any two of these three: a start time, a duration, and an end time.</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element name="startTime" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Start DateTime of the Time Period</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="duration" type="xsd:duration" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Duration of the Time Period. For additional information on the duration data type see:  <link href="http://www.w3schools.com/schema/schema_dtypes_date.asp"/>
					                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="endTime" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>End Date Time of the Time Period</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="DatePeriod">
		          <xsd:annotation>
			            <xsd:documentation>A span of time defined by any two of these three: a start date, a duration, and an end date.</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element name="startDate" type="xsd:date" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Start Date of the Date Period</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="duration" type="xsd:duration" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Duration of the Date Period. For additional information on the duration data type see:  <link href="http://www.w3schools.com/schema/schema_dtypes_date.asp"/>
					                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="endDate" type="xsd:date" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>End Date of the Date Period</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="Measure">
		          <xsd:simpleContent>
			            <xsd:extension base="xsd:decimal">
				              <xsd:attribute name="measurementUnit" type="xsd:normalizedString">
					                <xsd:annotation>
						                  <xsd:documentation>Unit of measure</xsd:documentation>
					                </xsd:annotation>
				              </xsd:attribute>
			            </xsd:extension>
		          </xsd:simpleContent>
	        </xsd:complexType>
         <xsd:complexType name="Quantity">
		          <xsd:simpleContent>
			            <xsd:extension base="xsd:decimal">
				              <xsd:attribute name="measurementUnit" type="xsd:normalizedString">
					                <xsd:annotation>
						                  <xsd:documentation>Unit of quantity</xsd:documentation>
					                </xsd:annotation>
				              </xsd:attribute>
			            </xsd:extension>
		          </xsd:simpleContent>
	        </xsd:complexType>
         <xsd:complexType name="Rate">
		          <xsd:annotation>
			            <xsd:documentation>Numeric information that is assigned or is determined by calculation, counting, or sequencing. It does not require a unit of quantity or unit of measure. Expressed as a quantity per quantity.</xsd:documentation>
		          </xsd:annotation>
		          <xsd:simpleContent>
			            <xsd:extension base="Quantity">
				              <xsd:attribute name="baseValue" type="xsd:decimal" use="optional">
					                <xsd:annotation>
						                  <xsd:documentation>The value of the denominator of the defintion of the rate.</xsd:documentation>
					                </xsd:annotation>
				              </xsd:attribute>
				              <xsd:attribute name="baseMeasurementUnit" type="xsd:normalizedString" use="optional">
					                <xsd:annotation>
						                  <xsd:documentation>The measurement unit of the denominator of the definition of the rate.</xsd:documentation>
					                </xsd:annotation>
				              </xsd:attribute>
			            </xsd:extension>
		          </xsd:simpleContent>
	        </xsd:complexType>
         <xsd:complexType name="ObjectKey">
		          <xsd:annotation>
			            <xsd:documentation>One or more ways of additionally identifying the object's instance. This is useful for 'natural
				keys', ID's that are defined or controlled by external authorities or are in general use (like a MSISDN or BAN or
				SOC code).
			</xsd:documentation>
		          </xsd:annotation>
		          <xsd:simpleContent>
			            <xsd:extension base="xsd:token">
				              <xsd:attribute name="keyName" type="eo:Identifier">
					                <xsd:annotation>
						                  <xsd:documentation>Key Qualifiers. The name of the key, within the usageContext. For example "BAN".
				</xsd:documentation>
					                </xsd:annotation>
				              </xsd:attribute>
				              <xsd:attribute name="usageContext" type="xsd:token">
					                <xsd:annotation>
						                  <xsd:documentation>The frame of reference where the key is applicable. For example: PREPAIDBILLING, WEBCATALOG
				</xsd:documentation>
					                </xsd:annotation>
				              </xsd:attribute>
			            </xsd:extension>
		          </xsd:simpleContent>
	        </xsd:complexType>
         <xsd:complexType name="Status">
		          <xsd:annotation>
			            <xsd:documentation>The state of an object at a given point in time, or time period.	</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element name="effectivePeriod" type="eo:TimePeriod" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Effective Period for the Status.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="description" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>An explanation of the status.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="reason" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The reason for the status. </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
		          <xsd:attribute name="statusCode" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>Representative label for the state of the object, during the time period defined in the status.  Recognized values are documented in the context where the status is used.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
		          <xsd:attribute name="subStatusCode" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>A finer division of statusCode. </xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
		          <xsd:attribute name="usageContext" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>The frame of reference where the status is applicable.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
	        </xsd:complexType>
         <xsd:complexType name="BalanceSummaryGroup">
		          <xsd:annotation>
			            <xsd:documentation>A logical grouping of balance information</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element name="balanceSummary" type="eo:BalanceSummary" minOccurs="0"
                            maxOccurs="unbounded">
			</xsd:element>
		          </xsd:sequence>
		          <xsd:attribute name="name" type="xsd:token"/>
	        </xsd:complexType>
         <xsd:complexType name="BalanceSummary">
		          <xsd:sequence>
			            <xsd:element name="typeCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The type of balance information given. For example, INITIAL, TRANSFERRED or REMAINING. See valid values in <link href="SubscriptionEnum/BalanceTypeCode"/>
					                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="description" type="eo:Description" minOccurs="0"/>
			            <xsd:element name="dueDate" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Captured the date associated with the subscription balance details (ie. payment due date).</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:choice>
				              <xsd:element name="amount" type="eo:Amount" minOccurs="0">
					                <xsd:annotation>
						                  <xsd:documentation>Captures the amount associated with the subscription balance type.</xsd:documentation>
					                </xsd:annotation>
				              </xsd:element>
				              <xsd:element name="quantity" type="eo:Quantity" minOccurs="0">
					                <xsd:annotation>
						                  <xsd:documentation>Captures the quantity associated with the subscription balance type, such as messages or MB.</xsd:documentation>
					                </xsd:annotation>
				              </xsd:element>
			            </xsd:choice>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="Description">
		          <xsd:annotation>
			            <xsd:documentation>A structure that provides a description for a certain usage and language. For example, a
				description in spanish for web display.
			</xsd:documentation>
		          </xsd:annotation>
		          <xsd:simpleContent>
			            <xsd:extension base="xsd:normalizedString">
				              <xsd:attribute name="language" type="xsd:token">
					                <xsd:annotation>
						                  <xsd:documentation>Identifier for the language used in the description.
						Value is the ISO 639-1 Code, a two letter code defined atIdentifier for the language used in the description.  Value is the ISO 639-1 Code, such as (en-US,en-CA,en-GB)defined at www.w3.org/standards </xsd:documentation>
					                </xsd:annotation>
				              </xsd:attribute>
				              <xsd:attribute name="usageContext" type="xsd:token">
					                <xsd:annotation>
						                  <xsd:documentation>The frame of reference where the description is applicable. For example WEB, MOBILE DEVICE.
						</xsd:documentation>
					                </xsd:annotation>
				              </xsd:attribute>
			            </xsd:extension>
		          </xsd:simpleContent>
	        </xsd:complexType>
         <xsd:complexType name="Contact">
		          <xsd:sequence>
			            <xsd:element name="personName" type="eo:PersonName" minOccurs="0"/>
			            <xsd:element name="status" type="eo:Status" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Recognized statusCode values are found in
 <link href="CommunicationEnum/ContactStatus"/>. </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="preference" type="eo:Preference" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Precedence, advantage, or choice of one contact over another</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="addressCommunication" type="eo:AddressCommunication" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Recognized usageContext values are found in
 <link href="CommunicationEnum/AddressUsage"/>. Recognized statusCode values are found in
 <link href="CommunicationEnum/AddressStatus"/>. Recognized subStatusCode values are found in
 <link href="CommunicationEnum/AddressSubStatus"/>
					                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="phoneCommunication" type="eo:PhoneCommunication" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Recognized usageContext values are found in
 <link href="CommunicationEnum/PhoneUsage"/>. Recognized statusCode values are found in
 <link href="CommunicationEnum/PhoneStatus"/>. </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="emailCommunication" type="eo:EmailCommunication" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Recognized usageContext values are found in
 <link href="CommunicationEnum/EmailUsage"/>. Recognized statusCode values are found in
 <link href="CommunicationEnum/EmailStatus"/>. Recognized subStatusCode values are found in
 <link href="CommunicationEnum/EmailSubStatus"/>
					                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="websiteCommunication" type="eo:WebsiteCommunication" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Recognized usageContext values are found in
 <link href="CommunicationEnum/WebsiteUsage"/>. Recognized statusCode values are found in
 <link href="CommunicationEnum/WebsiteStatus"/>. Recognized subStatusCode values are found in
 <link href="CommunicationEnum/WebsiteSubStatus"/>. </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="e911AddressCommunication" type="eo:AddressCommunication" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>e911 Address information that client provides to provision in downstream systems. </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="preferredLanguage" type="eo:PreferredLanguage" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>The preferred language of communication in the specified usage context. ISO 639-1 Code (en-US,en-CA,en-GB)</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="AddressCommunication">
		          <xsd:annotation>
			            <xsd:documentation>
Recognized "usageContext" values are found in
 <link href="CommunicationEnum/AddressUsage"/>
			            </xsd:documentation>
		          </xsd:annotation>
		          <xsd:complexContent>
			            <xsd:extension base="Communication">
				              <xsd:sequence>
					                <xsd:element name="address" type="eo:Address" minOccurs="0"/>
					                <xsd:element name="locationInfo" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>Normalized address information</xsd:documentation>
						                  </xsd:annotation>
						                  <xsd:complexType>
							                    <xsd:sequence>
								                      <xsd:element name="houseNumber" type="xsd:token" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Address House number.</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="streetDirection" type="xsd:token" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>The address street direction (e.g., east, west. etc.).</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="streetName" type="xsd:normalizedString" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Address Street Name.</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="streetSuffix" type="xsd:token" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>The street type (e.g., Street, Avenue, Grove, etc.) to be checked against a decoding table.</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="trailingDirection" type="xsd:token" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>The address trailing street direction (e.g., east, west. etc.).</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="apartmentTypeCode" type="xsd:token" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Apartment Designator.</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="apartmentNumber" type="xsd:token" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>The location of the flat/apartment within the building.</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
							                    </xsd:sequence>
						                  </xsd:complexType>
					                </xsd:element>
				              </xsd:sequence>
			            </xsd:extension>
		          </xsd:complexContent>
	        </xsd:complexType>
         <xsd:complexType name="PhoneCommunication">
		          <xsd:annotation>
			            <xsd:documentation>Recognized "usageContext" values are found in
 <link href="CommunicationEnum/PhoneUsage"/>
			            </xsd:documentation>
		          </xsd:annotation>
		          <xsd:complexContent>
			            <xsd:extension base="Communication">
				              <xsd:sequence>
					                <xsd:element name="phoneType" type="xsd:token" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>Indicates the type of Phone Communication. Recognized "usageContext" values are found in <link href="CommunicationEnum/PhoneType"/>
							                    </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="phoneNumber" type="xsd:normalizedString" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>The complete number without separation between 	country code, area code etc
							</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="countryCode" type="xsd:token" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>The country dialing code for a communication number
							</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="areaCode" type="xsd:token" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>The area dialing code for a communication number
							</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="localNumber" type="xsd:normalizedString" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>The communication number, not including country dialing or area dialing codes
							</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="phoneExtension" type="xsd:normalizedString" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>The extension of the associated communication
number
							</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="accessCode" type="xsd:normalizedString" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>The text that permits access to the electronic network of the associated ommunication number such as telephone network, for example 9, *70.
							</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="serviceUsageType" type="xsd:token" minOccurs="0" maxOccurs="unbounded">
						                  <xsd:annotation>
							                    <xsd:documentation>Indicates what type of phone service is governed by this Communication. Example, SMS, MMS Recognized "usageContext" values are found in <link href="CommunicationEnum/ServiceUsageType"/>
							                    </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
				              </xsd:sequence>
			            </xsd:extension>
		          </xsd:complexContent>
	        </xsd:complexType>
         <xsd:complexType name="EmailCommunication">
		          <xsd:annotation>
			            <xsd:documentation>Recognized "usageContext" values are found in <link href="CommunicationEnum/EmailUsage"/>
			            </xsd:documentation>
		          </xsd:annotation>
		          <xsd:complexContent>
			            <xsd:extension base="Communication">
				              <xsd:sequence>
					                <xsd:element name="emailAddress" type="xsd:normalizedString" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>URI in the case of email communication
							</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="emailFormat" type="xsd:token" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>Indicates that the communication is preferred
								in
								HTML format, Plain Text, Rich Text.
							</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
				              </xsd:sequence>
			            </xsd:extension>
		          </xsd:complexContent>
	        </xsd:complexType>
         <xsd:complexType name="WebsiteCommunication">
		          <xsd:annotation>
			            <xsd:documentation>Recognized "usageContext" values are found in
 <link href="CommunicationEnum/WebsiteUsage"/>
			            </xsd:documentation>
		          </xsd:annotation>
		          <xsd:complexContent>
			            <xsd:extension base="Communication">
				              <xsd:sequence>
					                <xsd:element name="websiteURI" type="xsd:anyURI" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>URI or URL for the communication
							</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
				              </xsd:sequence>
			            </xsd:extension>
		          </xsd:complexContent>
	        </xsd:complexType>
         <xsd:complexType name="Communication">
		          <xsd:sequence>
			            <xsd:element name="usageContext" type="xsd:token" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Definition of the what the communication should be used for. Valid values depend on the communication type. For phones see <link href="CommunicationEnum/PhoneUsage"/>. For email see <link href="CommunicationEnum/EmailUsage"/>. For addresses see <link href="CommunicationEnum/AddressUsage"/>. For websites see <link href="CommunicationEnum/WebsiteUsage"/>.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="preference" type="eo:Preference" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Precedence, advantage, or choice of one communication over another</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="effectivePeriod" type="eo:TimePeriod" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The segment of time when the communication is valid/available.					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="privacyProfile" type="eo:PrivacyProfile" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Privacy controls for this communication, indicating the circumstances in which this communication should be
used.
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="status" type="eo:Status" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Status of the communication: might be VALIDATED with a substatus of OVERWRITTEN. Same communication might also have a status ACTIVE or UNDELIVERABLE. UNDELIVERABLE might have a substatus of HARDBOUNCE or SOFTBOUNCE.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="specificationValue" type="eo:SpecificationValue" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Additional information about the communication, stored as name value pairs.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="PrivacyProfile">
		          <xsd:sequence>
			            <xsd:element name="optOut" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Indicates the opt out option.
If true, this communication cannot be used for telemarketing calls, mails, emails...etc. Recognized "optOut" values are found in
 <link href="CommunicationEnum/OptOut"/>
					                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="reason" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Indicates the reason for applying the Opt Out, if any.
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="activityType" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Indicates the type of marketing activity that
the customer has opted out from. If no value is provided, it is assumed that the customer has opted out from all marketing activity through this communication channel. Recognized values are found in
 <link href="CommunicationEnum/PrivacyProfileActivityType"/>
					                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="Preference">
		          <xsd:sequence>
			            <xsd:element name="preferred" type="xsd:boolean" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Indicator if this is a preferred communication. If false, this communication should be less used.
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="ranking" type="xsd:int" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Priority of ranking of the associated object or component. The lower the number, the higher the priority.
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="Address">
		          <xsd:sequence>
			            <xsd:element name="addressFormatType" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>A code specifying the format of this address(STREET_ADDRESS, POBOX_ADDRESS,MILITARY_ADDRESS,FOREIGN_ADDRESS,HWY_ADDRESS,RURALROUTE_ADDRESS)</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="addressLine1" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The first free form line, expressed as text, of an address</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="addressLine2" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The first second form line, expressed as text, of an address</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="addressLine3" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The third free form line, expressed as text, of an address</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="addressLine4" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The fourth free form line, expressed as text, of an address</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="building" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>A building name or number. An office park or campus may have several buildings that are distinguished by the building name or number.
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="floor" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>A floor in a building
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="area" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Refers to an area of the building such as left wing, right wing etc.  
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="cityName" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The name, expressed as text, of the city, town or village of this address</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="stateName" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Name of the state in which  the city falls, where applicable</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="stateCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Code of the state in which  the city falls, where applicable(WA, TX, GA)</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="provinceName" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Name of the province where the address is located, if applicable</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="provinceCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Code of the province where the address is located, if applicable(OX,PQ)</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="ruralRoute" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Rural Route Number for the address, if applicable</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="countyName" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>County Name, if applicable</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="countryCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The unique identifier of a country for this address (Reference ISO 3166 and UN/ECE Rec 3)</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="countryName" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The name of a country for this address (Reference ISO 3166 and UN/ECE Rec 3)</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="countrySubDivisionID" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>An identification of a country sub-division of this address such as a department in France. To be used only if there is no named term that represents the countrysubdivision concept being expressed</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="citySubDivisionName" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>A name, expressed as text, of a sub-division of a city for this address, for example a district or borough.  To be used only if there is no named term that represents the citysubdivision concept being expressed</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="attentionOf" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The name, expressed as text, of a person or department in the organization to whom incoming mail is marked with words such as 'for the attention of' or 'FAO' or 'ATTN' for this address</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="careOf" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The name, expressed as text, of a person or organization at this address to whom incoming mail is marked with words such as 'care of' or 'C/O'</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="postalCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>A code specifying the postalcode of the address</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="postalCodeExtension" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>An extension to the postalcode of the address, e.g., in 94070-3541, the extension is 3541.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="geoCodeID" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>A unique code that represents a geospatial coordinate measurement of an exact geographic location and time at, below, or above the surface of the earth</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="uncertaintyIndicator" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Uncertainty code for geoCode: 'FULL'-Full Address, 'ZIP'-Rural Zip+4, 'E'-Rural Zip+2, 'RURAL'-Rural Zip, 'D'-Street Zip+4, 'L'-Street Zip+2, 'STREET'-Street zip</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="inCityLimitIndicator" type="xsd:boolean" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Indicates whether or not this address is within the taxing boundary of the jurisdiction that's provided in the address.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="timeZoneCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Code indicating the time zone of the address, e.g., EST, PST, UTC-8, UTC-5</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="geographicCoordinates" minOccurs="0">
				              <xsd:complexType>
					                <xsd:sequence>
						                  <xsd:element name="latitude" type="xsd:normalizedString" minOccurs="0"/>
						                  <xsd:element name="longitude" type="xsd:normalizedString" minOccurs="0"/>
					                </xsd:sequence>
				              </xsd:complexType>
			            </xsd:element>
			            <xsd:element name="countyCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>A numeric code identifying the county from the Federal Information Processing Standard (FIPS).</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="NormalizedAddress">
		          <xsd:complexContent>
			            <xsd:extension base="Address">
				              <xsd:sequence>
					                <xsd:group ref="ParsedAddress"/>
				              </xsd:sequence>
			            </xsd:extension>
		          </xsd:complexContent>
	        </xsd:complexType>
         <xsd:complexType name="PersonName">
		          <xsd:sequence>
			            <xsd:element name="nameType" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Type of name such as Legal name, official name and so on.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="fullName" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The person's full name.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="firstName" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Is the Persons given name or first name</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="middleName" type="xsd:normalizedString" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Is the persons middle name or initial.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="familyName" type="xsd:normalizedString" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Is the persons family name</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="maidenName" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>A married name is the family name adopted by a person upon marriage, and in speaking of the many cultures where the practice is traditional for women, the maiden name is the family name that the married name replaces.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="title" type="xsd:normalizedString" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Is the title of the person. For example Dr, Senator, Mr..</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="salutation" type="xsd:normalizedString" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Way the person is addressed e.g. Honorable etc?</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="prefix" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Prefix to the last name that is not considered part of the last name, such as German von or French dela, etc.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="suffix" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Suffix to the last name, such as Senior, III, the Third etc. 
</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="militaryRank" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Military Rank 
</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="preferredName" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Name by which the person prefers to be addressed</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="aliasName" type="xsd:normalizedString" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Alias by which the person is known</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="effectivePeriod" type="eo:TimePeriod" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>A period of time for which the name is effective.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="PreferredLanguage">
		          <xsd:annotation>
			            <xsd:documentation>The preferred language of communication in the specified usage context. ISO 639-1 Code (en-US,en-CA,en-GB)
			</xsd:documentation>
		          </xsd:annotation>
		          <xsd:simpleContent>
			            <xsd:extension base="xsd:token">
				              <xsd:attribute name="usageContext"/>
			            </xsd:extension>
		          </xsd:simpleContent>
	        </xsd:complexType>
         <xsd:complexType xmlns:ns1="http://esp.t-mobile.com/2012/eo" name="SpecificationGroup">
		          <xsd:annotation>
			            <xsd:documentation>Provides the opportunity to have additional details about an object, beyond the attributes that are already explicitly defined as individual elements.  The SpecificationGroup groups one or more data attributes under a single name.  The group name is defined in the SpecificationGroup's name attribute.</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element name="specificationValue" type="eo:SpecificationValue" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>A specification is a property of an object that can be assigned a value.  SpecificationValue provides a structure to contain   additional details about an object, beyond the attributes that are already defined as concrete, named elements.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
		          <xsd:attribute name="name" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>The name of the group of specifications.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
	        </xsd:complexType>
         <xsd:complexType xmlns:ns1="http://esp.t-mobile.com/2012/eo" name="SpecificationValue">
		          <xsd:annotation>
			            <xsd:documentation>A specification is a property of an object that can be assigned a value.</xsd:documentation>
		          </xsd:annotation>
		          <xsd:simpleContent>
			            <xsd:extension base="xsd:token">
				              <xsd:attribute name="name" type="xsd:token">
					                <xsd:annotation>
						                  <xsd:documentation>The name of the Specification value; a label by which the value is commonly known.  For example a name = "LongDescription" for a value that contains a long description.</xsd:documentation>
					                </xsd:annotation>
				              </xsd:attribute>
				              <xsd:attribute name="dataType" type="xsd:token">
					                <xsd:annotation>
						                  <xsd:documentation>The data type of the value.</xsd:documentation>
					                </xsd:annotation>
				              </xsd:attribute>
				              <xsd:attribute name="measurementUnit" type="xsd:token">
					                <xsd:annotation>
						                  <xsd:documentation>The unit of measure of the value.</xsd:documentation>
					                </xsd:annotation>
				              </xsd:attribute>
			            </xsd:extension>
		          </xsd:simpleContent>
	        </xsd:complexType>
         <xsd:complexType xmlns:ns1="http://esp.t-mobile.com/2012/eo" name="Note">
		          <xsd:sequence>
			            <xsd:element name="entryTime" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Date and time when the note was entered/created.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="updateTime" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Date and time when the note was modified after the initial entry.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="dealerServiceCode" type="xsd:token" minOccurs="0"/>
			            <xsd:element name="noteType" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>An extensive list of note types (500+) are defined for systematic use. Business rules determine which note to apply in a given situation.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="systemText" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Predefined text associated with the note type. May have parameters filled in when instantiated.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="manualText" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Addiitonal descriptive information entered by the author to supplement the system text.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="channel" type="xsd:token" minOccurs="0"/>
			            <xsd:element name="author" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Identifies the author of the note.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
		          <xsd:attribute name="id" type="eo:Identifier">
			            <xsd:annotation>
				              <xsd:documentation>Unique identifier assigned by the system of record.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
		          <xsd:attribute name="language" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>Identifier for the language used in the description.  Value is the ISO 639-1 Code, a two letter code such as   (en-US,en-CA,en-GB)defined at www.w3.org/standards </xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
	        </xsd:complexType>
         <xsd:complexType name="ProductBusinessObjectProfile">
		          <xsd:annotation>
			            <xsd:documentation>Defines the base attributes of a product object.</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element name="key" type="eo:ObjectKey" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Alternate identifiers for the product or offer.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="name" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The label by which the product (or offer) is known across the T-Mobile enterprise. This is a
						user-friendly name, appropriate for display. This should always be populated if possible, as this is the place
						applications will look for a name to display to users.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="shortName" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Same as the name of the product (or offer), shortened for uses where name is too long. For
						example, mobile phone or tablet.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="description" type="eo:Description" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>The primary description of the product (or offer). There may be multiple descriptions, one per
						language. This should always be populated if possible, as this is the place applications will look for a
						"description" to display to users.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="shortDescription" type="eo:Description" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Same as the description of the product (or offer), shortened for uses where description is too
						long. For example, mobile phone or tablet. There may be multiple shortDescriptions, one per language.
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="alternateDescription" type="eo:Description" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>A list of descriptions in other systems. Can be in multiple languages for the same system.
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
		          <xsd:attribute name="id" type="eo:Identifier">
			            <xsd:annotation>
				              <xsd:documentation>Unique identifier assigned by the system of record.
				</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
		          <xsd:attribute name="version" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>The version of the object in the enterprise product catalog.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
		          <xsd:attribute name="versionStartDate" type="xsd:dateTime">
			            <xsd:annotation>
				              <xsd:documentation>The date this version of the object  is effective in the enterprise product catalog.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
		          <xsd:attribute name="versionEndDate" type="xsd:dateTime">
			            <xsd:annotation>
				              <xsd:documentation>The date this version of the object  is no longer effective in the enterprise product catalog.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
		          <xsd:attribute name="actionCode" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>Indicates the type of change that has been made to this version, as compared to the previous version.  Recognized values are found in <link href="ProductEnum/ActionCode"/>
				              </xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
	        </xsd:complexType>
         <xsd:complexType name="BaseRequest" abstract="true">
		          <xsd:annotation>
			            <xsd:documentation>The basic type that should be extended for a request.</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element name="header" type="eo:Header" minOccurs="1"/>
		          </xsd:sequence>
		          <xsd:attribute name="serviceTransactionId" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>
					Unique tracking value generated by the system and
					returned at end of transaction. It may be sent in
					subsequent requests if it was returned from an incomplete
					operation. This allows the sender to poll for final
					results when the operation is asynchronous.
				</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
	        </xsd:complexType>
         <xsd:complexType name="BaseResponse" abstract="true">
		          <xsd:annotation>
			            <xsd:documentation>The basic type that should be extended for a response.</xsd:documentation>
		          </xsd:annotation>
		          <xsd:complexContent>
			            <xsd:extension base="BaseRequest">
				              <xsd:sequence>
					                <xsd:element name="responseStatus" type="eo:ResponseStatus" minOccurs="1"/>
				              </xsd:sequence>
			            </xsd:extension>
		          </xsd:complexContent>
	        </xsd:complexType>
         <xsd:complexType name="Header">
		          <xsd:annotation>
			            <xsd:documentation>
				Metadata for every POX and SOAP transaction. Data in the Header
				is used to track and configure
				service operations. It's supplied
				by the Sender and returned in the response for continuity and
				to return values set
				by the system.
			</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>

			            <xsd:element name="sender" type="eo:Sender" minOccurs="1">
				              <xsd:annotation>
					                <xsd:documentation>
						A description of the actor that is calling a service operation.
						This data is supplied by the
						calling client at the time of the request.
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>

			            <xsd:element name="target" type="eo:Target" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>
						Data that the target system can use to configure the operation.
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>

		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="Sender">
		          <xsd:sequence>

			            <xsd:element name="senderId" type="xsd:token">
				              <xsd:annotation>
					                <xsd:documentation>
						Uniquely identifies an Operation consumer.
						Used for Tracking and in Business Rules.
						<p>Examples include:</p>
						                  <ul>
							                    <li>BESTBUY</li>
							                    <li>MYTMO</li>
							                    <li>TMO_IHAP</li>
							                    <li>TMO_ENG</li>
							                    <li>TMO_EIT</li>
							                    <li>WALMART FAMILY MOBILE</li>
							                    <li>WALMART</li>
						                  </ul>
					                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>

			            <xsd:element name="channelId" type="xsd:token">
				              <xsd:annotation>
					                <xsd:documentation>
						Identifies the business unit or sales channel for the Request.
						Property values relating to the target system are
						derived using
						partner, application, and channel information. This value may
						control business logic and data routing.
						<p>Examples include:</p>
						                  <ul>
							                    <li>RETAIL</li>
							                    <li>CARE</li>
							                    <li>STORE</li>
							                    <li>ONLINE</li>
							                    <li>HANDSET</li>
							                    <li>SELFCARE</li>
							                    <li>SIDEDOOR</li>
						                  </ul>
					                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>

			            <xsd:element name="applicationId" type="xsd:token">
				              <xsd:annotation>
					                <xsd:documentation>
						Identifies the application, system, or tool that is being used to
						perform the transaction(s) related to this
						request. Property values
						relating to the target system are derived using partner, application,
						and channel
						information.
						<p>Examples include:</p>
						                  <ul>
							                    <li>CAM</li>
							                    <li>QUICKVIEW</li>
							                    <li>TMODOTCOM</li>
							                    <li>TIBCO</li>
							                    <li>HSO</li>
							                    <li>MYACCOUNT</li>
						                  </ul>
					                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>

			            <xsd:element name="applicationUserId" type="xsd:token" minOccurs="0"/>

			            <xsd:element name="sessionId" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>
						A value populated by the Sender used to track the transactions
						that occur during a session, a
						long-lasting interaction that is
						maintained by the sender.
						Senders are encouraged to develop a
						useful scheme.
						Do not use customer-sensitive information.
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>

			            <xsd:element name="workflowId" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>
						A value populated by the Sender used to track their workflow
						within the confines of a session.
						Senders are encouraged to develop a
						useful scheme.
						Do not use customer-sensitive information.
							</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>

			            <xsd:element name="activityId" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>
						A value populated by the Sender used to track a unit of work
						within a workflow.
						Senders are
						encouraged to develop a
						useful scheme.
						Do not use customer-sensitive information.
							</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>

			            <xsd:element name="timestamp" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>
						A timestamp provided by Sender to track their workflow.
						Usually set to the moment when a request
						is made.
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>

			            <xsd:element name="storeId" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>
						For tracking purposes only. Unique identifier for the store.
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>

			            <xsd:element name="dealerCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>
						For tracking purposes only. Code that uniquely identifies the dealer.
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>

		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="Target">
		          <xsd:sequence>
			            <xsd:element name="targetSystemId" type="eo:TargetSystemIdentity" minOccurs="0"
                            maxOccurs="unbounded"/>
			            <xsd:element name="userCompanyId" type="xsd:token" minOccurs="0"/>
			            <xsd:element name="customerCompanyId" type="xsd:token" minOccurs="0"/>
			            <xsd:element name="servicePartnerId" type="xsd:token" minOccurs="0"/>
			            <xsd:element name="transactionType" type="xsd:token" minOccurs="0"/>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="TargetSystemIdentity">
		          <xsd:all>
			            <xsd:element name="systemId" type="xsd:token"/>
			            <xsd:element name="userId" type="xsd:token"/>
		          </xsd:all>
	        </xsd:complexType>
         <xsd:complexType name="TargetSystemParameter">
		          <xsd:sequence>
			            <xsd:element name="value" type="xsd:token">
				              <xsd:annotation>
					                <xsd:documentation>
						The value of the parameter being sent to the target system.
		        	</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
		          <xsd:attribute name="name" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>
					The name of the parameter being sent to the target system.
		        </xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
	        </xsd:complexType>
         <xsd:complexType name="ResponseStatus">
		          <xsd:sequence>
			            <xsd:element name="detail" type="eo:ResponseStatusDetail" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>
						Status details that describe one or more contributions to the overall response code.
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
		
		          <xsd:attribute name="code" type="eo:ResponseStatusCode"/>
		
	        </xsd:complexType>
         <xsd:complexType name="ResponseStatusDetail">
		          <xsd:sequence>
			            <xsd:element name="definition" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>
						The generic definition of subStatusCode.
                    </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="explanation" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>
						Specific information about the data or circumstances that explains why this status detail is part of the response.
						It should provide the details aid in troubleshooting and taking further action.
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>

		          <xsd:attribute name="subStatusCode" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>
					A numeric value that defines the status detail, like an error code.
					This code allows client systems to programmatically handle the status detail, if desired.
		        </xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>

	        </xsd:complexType>
         <xsd:complexType name="SubscriptionExtension">
		          <xsd:complexContent>
			            <xsd:extension base="Subscription">
				              <xsd:sequence>
					                <xsd:element name="installedProduct" type="eo:InstalledProduct" minOccurs="0"
                                  maxOccurs="unbounded">
						                  <xsd:annotation>
							                    <xsd:documentation>Installed products that are employed to provide service to the subscription. Includes equipment and services.</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="subscriptionPromotion" type="eo:SubscriptionPromotion" minOccurs="0"
                                  maxOccurs="unbounded">
						                  <xsd:annotation>
							                    <xsd:documentation>Promotions associated with this subscription.</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="balanceSummaryGroup" type="eo:BalanceSummaryGroup" minOccurs="0"
                                  maxOccurs="unbounded">
						                  <xsd:annotation>
							                    <xsd:documentation>Balance information for the subscription</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="specificationGroup" type="eo:SpecificationGroup" minOccurs="0"
                                  maxOccurs="unbounded">
						                  <xsd:annotation>
							                    <xsd:documentation>Represents the grouping of user defined properties of the subscription. Recognized values are found in <link href="SubscriptionEnum/SpecificationGroupName"/> and <link href="SubscriptionEnum/SpecificationValueName"/>
							                    </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="activityHistory" type="eo:ActivityInstance" minOccurs="0"
                                  maxOccurs="unbounded">
						                  <xsd:annotation>
							                    <xsd:documentation>Recognized values for activityName are found in <link href="SubscriptionEnum/ActivityName"/>
							                    </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
				              </xsd:sequence>
			            </xsd:extension>
		          </xsd:complexContent>
	        </xsd:complexType>
         <xsd:complexType name="Subscription">
		          <xsd:complexContent>
			            <xsd:extension base="SubscriptionProfile">
				              <xsd:sequence>
					                <xsd:element name="customerID" type="eo:Identifier" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>Uniquely identifies the customer that uses this subscription. Not necessarily the account holder.</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="subscriberContact" type="eo:Contact" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>Defines methods for communicating with the subscriber. See valid values for subscriber status in <link href="SubscriptionEnum/SubscriberStatus"/>
							                    </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="note" type="eo:Note" minOccurs="0" maxOccurs="unbounded">
						                  <xsd:annotation>
							                    <xsd:documentation>Note concerning the subscription.</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
				              </xsd:sequence>
			            </xsd:extension>
		          </xsd:complexContent>
	        </xsd:complexType>
         <xsd:complexType name="InstalledProduct">
		          <xsd:annotation>
			            <xsd:documentation>An installed product defines a product or service instance deployed by a corporation or business. It includes reference to parties who may have an interest arising from ownership/lease, usage, maintenance management, billing etc. It is a snapshot of the definition of the product or service instance which includes relationship (like component, substitute, etc.) to other installed products. However, it doesn't include operations or change in value during the lifecycle of the product or service.</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element name="key" type="eo:ObjectKey" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Alternate identifiers for the installed product.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="product" type="eo:ProductProfile" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Identifies the product in the product catalog.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="status" type="eo:Status" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Identifies the status of an installed product. The status helps to process and service an installed product properly.
                               </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="processingStatus" type="eo:Status" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Status of the processing of this installed product (e.g., Installed, In-transit, Upgraded, Returned for repair etc.)</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="registrationStatus" type="eo:Status" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Status of the registration of this installed product (e.g., Registered-Approved, Registered-Not Approved, Not Registered etc.)           </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="unitPurchasePrice" type="eo:Amount" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
The price paid per unit of the installed product
                               </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="effectivePeriod" type="eo:EffectivePeriod" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation> 
The effective period when the installed product is valid
                               </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="customerOwnedIndicator" type="xsd:boolean" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Indicates whether the customer is the owner of the installed product. In the case of a lease, the installed product should be owned by a business unit within the deploying enterprise
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="installationTime" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
The datetime when the installed product was installed
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="purchaseTime" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
The datetime when the installed product was purchased
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="manufacturedDate" type="xsd:date" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
The date when the installed product was manufactured
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="serialNumber" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Unique serial number for identifying an installed product - this is applicable only for installed products based on a serializable product. Quantity will always be 1 if serial number is specified
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="parentInstalledProduct" type="eo:Identifier" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Reference to the parent installed product</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="componentInstalledProduct" type="eo:Identifier" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>The components that make up the installed product. The components are installed product themselves.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="relatedInstalledProduct" type="eo:Identifier" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Other installed products to which the installed product may be related - the relationship may be a substitute/backup etc.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="specificationGroup" type="eo:SpecificationGroup" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Represents the grouping of user defined properties of the installed product. Recognized values are found in <link href="InstalledProductEnum/SpecificationGroupName"/> and <link href="InstalledProductEnum/SpecificationValueName"/>
					                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="processingHistory" type="eo:ActivityInstance" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Recognized values are found in <link href="InstalledProductEnum/ActivityName"/>
					                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
		          <xsd:attribute name="id" type="eo:Identifier">
			            <xsd:annotation>
				              <xsd:documentation>Unique identifier assigned by the system of record.
				</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
		          <xsd:attribute name="actionCode" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>Indicates the type of change that has been made to this installed product, as compared to the existing configuration.  Recognized values are found in <link href="InstalledProductEnum/ActionCode"/>
				              </xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
	        </xsd:complexType>
         <xsd:complexType name="EffectivePeriod">
		          <xsd:annotation>
			            <xsd:documentation>The effective period for the installed product. The period may define the duration for a warning period associated with a service.</xsd:documentation>
		          </xsd:annotation>
		          <xsd:complexContent>
			            <xsd:extension base="TimePeriod">
				              <xsd:sequence>
					                <xsd:element name="usageContext" type="xsd:token" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>Recognized values are found in <link href="InstalledProductEnum/EffectivePeriodUsageContext"/>
							                    </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
				              </xsd:sequence>
			            </xsd:extension>
		          </xsd:complexContent>
	        </xsd:complexType>
         <xsd:complexType name="ProductProfile">
		          <xsd:annotation>
			            <xsd:documentation>A brief summarization of a product.
			</xsd:documentation>
		          </xsd:annotation>
		          <xsd:complexContent>
			            <xsd:extension base="ProductBusinessObjectProfile">
				              <xsd:sequence>
					                <xsd:element name="productCategory" type="xsd:token" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>The product classification, from a model perspective.  E.g., RatePlan, Service, Device.  This list is governed by the owners of the T-Mobile Common Product Model.  Recognized values are found in <link href="ProductEnum/ProductCategory"/>.	</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="characteristic" type="eo:SpecificationGroup" minOccurs="0"
                                  maxOccurs="unbounded">
						                  <xsd:annotation>
							                    <xsd:documentation>Describes the trait or capabilities of the product. For example,
whether or not a rateplan is eligible for customer loyalty rewards program.  Recognized values are found in <link href="ProductEnum/ProductCharacteristicNameX"/> , where X is the product category.</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="classification" type="eo:Classification" minOccurs="0"
                                  maxOccurs="unbounded">
						                  <xsd:annotation>
							                    <xsd:documentation>Categorizes products according to common attributes. For example,
a rateplan product is categorized by the type(s) of service it includes; VOICE, DATA, TEXT or some combination.  Recognized names of classifications are found in <link href="ProductEnum/ProductClassificationCode"/>.</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="productStatus" type="eo:Status" minOccurs="0" maxOccurs="unbounded">
						                  <xsd:annotation>
							                    <xsd:documentation>The status of the product in the catalog.  For example the statusCode of ACTIVE/INACTIVE.   Recognized values of the statusCode  are found in <link href="ProductEnum/ProductStatus"/>.  subStatusCode is reserved for future use.</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="catalog" type="xsd:token" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation> 
The name of the catalog to which the Product belongs.   For example TMOBILE, SECONDBRAND, WHOLESALE.  Recognized values  are found in <link href="ProductEnum/CatalogName"/>.           </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
				              </xsd:sequence>
			            </xsd:extension>
		          </xsd:complexContent>
	        </xsd:complexType>
         <xsd:complexType name="Classification">
		          <xsd:annotation>
			            <xsd:documentation>An organization of objects into groups based on common attributes.  As an example a supermarket may have classifications such as Grocery, Cleaning Supplies, Personal Care etc. Grocery may have child classifications such as Produce, Dairy, Beverages etc. </xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element name="key" type="eo:ObjectKey" minOccurs="0" maxOccurs="unbounded"/>
			            <xsd:element name="classificationRelationship" type="eo:ClassificationRelationship"
                            minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Other Classification objects related to this instance.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="classificationCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The label that describes this category. For example the BillingArrangementType.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="classificationAttribute" type="eo:SpecificationValue" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Classifications may have multiple attributes; for example Name, Description, Code, EffectiveTimePeriod. Each attribute is represented as an AttributeName/AttributeValue pair, therefore many	AttributeName/AttributeValue pairs can be attached to this object instance.  The classificationAttribute holds the 'value' part of the key/value pair.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="classificationStatus" type="eo:Status" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>State of the classification value  during the time period defined in the status.  For example, effective or not effective.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
		          <xsd:attribute name="id" type="eo:Identifier">
			            <xsd:annotation>
				              <xsd:documentation>Unique identifier assigned by the system of record.
				</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
		          <xsd:attribute name="versionStartDate" type="xsd:dateTime">
			            <xsd:annotation>
				              <xsd:documentation>The date this version of the object  is effective in the enterprise product catalog.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
		          <xsd:attribute name="versionEndDate" type="xsd:dateTime">
			            <xsd:annotation>
				              <xsd:documentation>The date this version of the object  is no longer effective in the enterprise product catalog.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
		          <xsd:attribute name="actionCode" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>Indicates the type of change that has been made to this version, as compared to the previous version.  Recognized values are found in <link href="ProductEnum/ActionCode"/>
				              </xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
		          <xsd:attribute name="version" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>The version of the object in the enterprise product catalog.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
	        </xsd:complexType>
         <xsd:complexType name="ClassificationRelationship">
		          <xsd:sequence>
			            <xsd:element name="relatedClassification" type="eo:RelatedClassification" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>The Classification that this Classification is related to.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
		          <xsd:attribute name="relationshipCode" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>Describes the type of relationship between the classifications. Recognized values are found in
<link href="ProductEnum/ClassificationRelationshipCode"/>.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
	        </xsd:complexType>
         <xsd:complexType name="RelatedClassification">
		          <xsd:annotation>
			            <xsd:documentation>Defines a relationship between Classifications.
			</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element name="classificationId" type="eo:Identifier" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The Classification that this Classification  is related to. For example the Classification FeatureSubCategory may have a parent Classification, FeatureCategory.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="relationshipStatus" type="eo:Status" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The Status represents the current condition of	the RelatedClassification instance. 	Recognized values are found in	<link href="ProductEnum/ClassificationRelationshipStatus"/>.
					</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="ActivityInstance">
		          <xsd:annotation>
			            <xsd:documentation>A business activity that causes a change in the status of the object. For example new service activation, feature addition, cancel service.</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element name="activityId" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Uniquely identifies the business activity definition.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="activityName" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The name of the business activity that caused the processing status of the object to change. For example: Activation, ChangeRatePlan</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="activityPeriod" type="eo:TimePeriod" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Time period that the activity occurred, is occurring or will occur</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="description" type="eo:Description" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Description of the processing activity.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="reason" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Code identifying the reason for the activity</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="status" type="eo:Status" minOccurs="0" maxOccurs="unbounded"/>
			            <xsd:element name="relatedActivity" type="eo:RelatedActivity" maxOccurs="unbounded"
                            minOccurs="0"/>
			            <xsd:element name="specificationValue" type="eo:SpecificationValue" minOccurs="0"
                            maxOccurs="unbounded"/>
		          </xsd:sequence>
		          <xsd:attribute name="id" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>Unique identifier assigned by the system of record for the occurrence of the business activity.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
	        </xsd:complexType>
         <xsd:complexType name="Activity">
		          <xsd:annotation>
			            <xsd:documentation>A business activity that causes a change in the status of the object. For example new service activation, feature addition, cancel service.</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element name="activityName" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The unique name of the business activity.  For example: Activation, ChangeRatePlan</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="activityPeriod" type="eo:TimePeriod" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Time period that the activity occurred, is occurring or will occur</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="description" type="eo:Description" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Description of the processing activity.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="reason" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Code identifying the reason for the activity</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="status" type="eo:Status" minOccurs="0" maxOccurs="unbounded"/>
			            <xsd:element name="relatedActivity" type="eo:RelatedActivity" minOccurs="0"
                            maxOccurs="unbounded"/>
			            <xsd:element name="specificationValue" type="eo:SpecificationValue" minOccurs="0"
                            maxOccurs="unbounded"/>
		          </xsd:sequence>
		          <xsd:attribute name="id" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>Uniquely identifies the business activity definition.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
	        </xsd:complexType>
         <xsd:complexType name="RelatedActivity">
		          <xsd:annotation>
			            <xsd:documentation>Defines a realationship between activities.		</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element name="activityName" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Name of the related activity.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="relationshipStatus" type="eo:Status" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The status of the relationship between the activities.	 For example, is the relationship currently valid or not.	</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
		          <xsd:attribute name="id" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>Uniquely identifies the related business activity definition.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
		          <xsd:attribute name="relationshipCode" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>Describes the type of relationship. Recognized values are found in<link href="ProductEnum/ActivityRelationshipCode"/>
				              </xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
	        </xsd:complexType>
         <xsd:complexType name="SubscriptionPromotion">
		          <xsd:sequence>
			            <xsd:element name="key" type="eo:ObjectKey" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Alternative identifiers for the application of this promotion to this subscription.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="promotionId" type="eo:Identifier" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Uniquely identifies the definition of the promotion being applied to this subscription.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="amount" type="eo:Amount" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The currency amount being applied to this subscription by this promotion.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="channel" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The channel through which this promotion was applied to the subscription.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="description" type="eo:Description" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>A text description of the promotion being applied to the subscription.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="effectivePeriod" type="eo:EffectivePeriod" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>The time period during which this promotion will be applied to this subscription.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="note" type="eo:Note" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Additional information about the way this promotion was applied to the subscription.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="status" type="eo:Status" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>The status of the promotion as it applies to this subscription.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="relatedProduct" type="eo:ProductProfile" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Products that influenced the eligibility of this subscription for this promotion.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="balanceSummary" type="eo:BalanceSummary" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Balances related to the promotion being applied to this subscription.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="activityHistory" type="eo:Activity" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Recognized values are found in <link href="SubscriptionPromotionEnum/ActivityName"/>
					                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="specificationGroup" type="eo:SpecificationGroup" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Represents the grouping of user defined properties of the Subscription Promotion. Recognized values are found in <link href="SubscriptionPromotionEnum/SpecificationGroupName"/> and <link href="SubscriptionPromotionEnum/SpecificationValueName"/>
					                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
		          <xsd:attribute name="id" type="eo:Identifier">
			            <xsd:annotation>
				              <xsd:documentation>Uniquely identifies the application of the promotion to this subscription.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
		          <xsd:attribute name="actionCode" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>Specifies the action to be taken with this promotion relative to this subscription. Recognized values are found in <link href="SubscriptionPromotionEnum/actionCode"/>
				              </xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
	        </xsd:complexType>
         <xsd:complexType name="Payment">
		          <xsd:annotation>
			            <xsd:documentation>Payment encapsulates a Payment transaction between one payer account and one payee account. The deploying organization acts as creditor for the purpose of this object and hence this Payment is incoming to the Accounts Receivable system of the deploying company. One  Payment transaction may pay more than one Accounts Receivable Document in creditor's system.</xsd:documentation>
		          </xsd:annotation>
		          <xsd:complexContent>
			            <xsd:extension base="PaymentProfile">
				              <xsd:sequence>
					                <xsd:element name="exclusiveIndicator" type="xsd:boolean" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation> 
Indicates whether this payment is made up of a single document receivable that was meant to be paid alone.
                                </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="declarationRequiredIndicator" type="xsd:boolean" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation> 
Indicates whether this payment needs to be reported to the central bank.
                                </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="finalIndicator" type="xsd:boolean" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation> 
Indicates whether the payment is final in a series of payments. Usage: The user knows whether any payment is pending.
                                </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="convertedRequestAmount" type="eo:Amount" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation> 
Request Amount converted to the Payment method base currency.
                                </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="allocationAmount" type="eo:Amount" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation> 
Total amount allocated to document receivables such as Invoices.
                                </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <!--				<xsd:element name="currencyExchange" type="CurrencyExchange" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation> 
Currency exchange rate between the payment transaction request currency and the payment method currency
                               </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
-->
					<xsd:element name="note" type="xsd:normalizedString" minOccurs="0" maxOccurs="unbounded">
						                  <xsd:annotation>
							                    <xsd:documentation>Comments regarding the payment.</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="payFromParty" type="eo:Contact" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation> 
Refers to the identification and address of the PayFrom Party
                               </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="payeeParty" type="eo:Contact" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation> 
Refers to the identification and address of the Payee Party
                               </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="externalCustomerParty" type="eo:Contact" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation> 
Captures information about an external customer (ie. payor who is not a customer).
                               </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="paymentRegulatoryReport" type="eo:PaymentRegulatoryReport" minOccurs="0"
                                  maxOccurs="unbounded">
						                  <xsd:annotation>
							                    <xsd:documentation> 
Information needed because of regulatory and statutory requirements
                               </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="stopPayment" type="eo:StopPayment" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation> 
Details of a Stop Payment Request
                               </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="voidPayment" type="eo:VoidPayment" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation> 
Details of a Void Payment Request
                               </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="releasePayment" type="eo:ReleasePayment" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation> 
Details of a Stop-Payment Release Request
                               </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="paymentLine" type="eo:PaymentLine" minOccurs="0" maxOccurs="unbounded">
						                  <xsd:annotation>
							                    <xsd:documentation> 
Line Items corresponding to accounts receivable Documents that are being paid by this Payment document
                               </xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:element name="paymentAttachment" type="eo:Attachment" minOccurs="0"
                                  maxOccurs="unbounded">
						                  <xsd:annotation>
							                    <xsd:documentation>Reference to additional documentation included with the payment.</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
					                <xsd:choice>
						                  <xsd:sequence>
							                    <xsd:element name="bankPayment" type="eo:BankPayment" minOccurs="0">
								                      <xsd:annotation>
									                        <xsd:documentation> 
Bank Account transaction details for the cases where payment method is : Direct Debit, Check or EFT
                               </xsd:documentation>
								                      </xsd:annotation>
							                    </xsd:element>
						                  </xsd:sequence>
						                  <xsd:sequence>
							                    <xsd:element name="creditCardPayment" type="eo:CreditCardPayment" minOccurs="0">
								                      <xsd:annotation>
									                        <xsd:documentation> 
Credit Card Payment and authorization Details
                               </xsd:documentation>
								                      </xsd:annotation>
							                    </xsd:element>
						                  </xsd:sequence>
						                  <xsd:sequence>
							                    <xsd:element name="debitCardPayment" type="eo:DebitCardPayment" minOccurs="0">
								                      <xsd:annotation>
									                        <xsd:documentation> 
Debit Card Payment and authorization Details
                               </xsd:documentation>
								                      </xsd:annotation>
							                    </xsd:element>
						                  </xsd:sequence>
					                </xsd:choice>
					                <xsd:element name="specificationGroup" type="eo:SpecificationGroup" minOccurs="0"
                                  maxOccurs="unbounded"/>
				              </xsd:sequence>
			            </xsd:extension>
		          </xsd:complexContent>
	        </xsd:complexType>
         <xsd:complexType name="PaymentLine">
		          <xsd:sequence>
			            <xsd:element name="creditIndicator" type="xsd:boolean" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Indicates whether this document receivable is being paid by credit.
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="reasonCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Purpose of this Document Receivable
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="allocationTypeCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Type indicating the specific purpose this allocated amount serves for the referred document receivable. Example values are : DuePayable, DiscountApplied, CreditNote, Remitted, Tax, Charges 
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="allocationAmount" type="eo:Amount" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Total payment receipt amount against this document receivable in Payment method currency
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="description" type="xsd:normalizedString" minOccurs="0"
                            maxOccurs="unbounded"/>
			            <xsd:element name="note" type="xsd:normalizedString" minOccurs="0" maxOccurs="unbounded"/>
			            <xsd:element name="DocumentReference" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Reference to the accounts receivable document. Examples are : Commercial Invoice, Credit Note etc. 
                               </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="BAN" type="xsd:normalizedString" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Billing Account Number (BAN) identifies the Customer Account that is being paid.
                               </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="ReceivedPaymentLineAttachment" type="xsd:normalizedString" minOccurs="0"
                            maxOccurs="unbounded"/>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="PaymentRegulatoryReport">
		          <xsd:sequence>
			            <xsd:element name="TypeCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Identifies whether the regulatory reporting information applies to the credit side or debit side or both of the transaction.
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="AuthorityName" type="xsd:normalizedString" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation> 
Name of the entity requiring the regulatory reporting information
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="ReasonCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Specifies the nature, purpose and reason for the transaction to be reported for regulatory and statutory requirements. 
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="ReportingAmount" type="eo:Amount" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Amount to be reported for regulatory and statutory requirements. 
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="AuthorityCountryCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Country of the entity requiring the regulatory reporting information
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="ReleasePayment">
		          <xsd:sequence>
			            <xsd:element name="ReleaseDateTime" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Date and time user released stop payment
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="ReasonCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Reason for releasing the stop request.
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="requesterContact" type="eo:Contact" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Customer contact that requested to release the stop-payment request
                               </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="StopPayment">
		          <xsd:sequence>
			            <xsd:element name="requestTime" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Date when stop  payment request was placed
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="reasonCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Reason for requesting stop payment.
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="requesterContact" type="eo:Contact" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Customer contact that requested stop payment
                               </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="VoidPayment">
		          <xsd:sequence>
			            <xsd:element name="voidTime" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Date when the payment request was voided.
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="reasonCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Reason for voiding the payment.
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="RequesterContact" type="eo:Contact" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Customer contact that requested to void this payment
                               </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="BankPayment">
		          <xsd:sequence>
			            <xsd:element name="actionTypeCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Type of EFT (Electronic Funds Transfer) transaction attempted.  Sample Values : DEBIT, CREDIT, VERIFY, VALIDATE</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="instructionCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Unique identification of the Bank payment Instruction that the PayFrom party instructs the bank</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="settlementPriorityCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The priority with which the financial institution or payment system should settle payment for this document</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="sequenceTypeCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Identifies the direct debit sequence i.e. First, Recurrent, Final or One-off etc</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="reasonCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Payment Reason code determining the purpose of the payment. Usage: This code helps to process and fulfill the payment properly</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="authorizationMethodCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Method by which the payer authorized the transaction.  Example of some values can be: WRITTEN, INTERNET_FORM</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="deliveryMethodCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Method by which the transaction is delivered.  Example of some values can be: ACH, FASCIMILE</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="transferTypeCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Bank account payment transfer type</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="referenceCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Payment system-provided reference code for this payment</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="chargeBearerCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Bearer of bank account charges.  Specifies which party/parties will bear the charges associated with the processing of the payment transaction. Example of some values can be: Credit, Debit, Shared, Service Level</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="chargeAmount" type="eo:Amount" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Bank charge amount in payer's Bank account currency</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="payFromBankAccount" type="eo:BankAccount" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Payer's Bank Account</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="payeeBankAccount" type="eo:BankAccount" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Payee's Bank Account</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="chargeProcessingBankAccount" type="eo:BankAccount" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Bank Account that pays the Bank Charge amount, if different from the Payer's Bank Account              </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="paymentDebitContact" type="eo:Contact" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Contact information related to the Debit Notification        </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="paymentRemittanceContact" type="eo:Contact" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Contact information related to the Remittance</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:choice>
				              <xsd:sequence>
					                <xsd:element name="serviceLevelCode" type="xsd:token" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>Agreement of Service between the payFrom party and the payee party under which the transfer should be processed. For SEPA the service level code must be SEPA. The other two options are EBA Priority Service and Same Day Value. Alternatively the company and the bank can use mutually agreed service level</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
				              </xsd:sequence>
				              <xsd:sequence>
					                <xsd:element name="clearingChannelCode" type="xsd:token" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>Clearing channel to be used for the transaction. Valid values are : Book Transfer, Mass Payment Net System, Real Time Gross Settlement System, Real Time Net Settlement System</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
				              </xsd:sequence>
			            </xsd:choice>
			            <xsd:choice>
				              <xsd:sequence>
					                <xsd:element name="paymentDirectDebitMandate" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>Mandate for the Direct Debit in case of a Direct debit payment</xsd:documentation>
						                  </xsd:annotation>
						                  <xsd:complexType>
							                    <xsd:sequence>
								                      <xsd:element name="authorizationMethodCode" type="xsd:token" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Mandate authorization method. Example values are : Phone, Web, Written etc</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="authorizedIndicator" type="xsd:boolean" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Flag indicating whether the payer has explicitly granted the mandate or not</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="signatureTime" type="xsd:dateTime" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Date on which the mandate has been signed by the payer party</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="electronicSignature" type="xsd:normalizedString" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Digital signature as provided by the payer party</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="frequencyCode" type="xsd:token" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Frequency of direct debit. Example values are :Daily, Weekly, Monthly etc.</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="amendmentIndicator" type="xsd:boolean" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Flag indicating whether the underlying mandate has been amended or not</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="effectiveTimePeriod" type="eo:TimePeriod" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>First Collection Date and Final Collection Date as authorized by the Mandate</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="mandateSignerPartyReference" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Credit Party that signs the Direct Debit Mandate              </xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="originalDirectDebitMandateReference" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Original Mandate Reference for the case where amendments are made to the original mandate</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
							                    </xsd:sequence>
						                  </xsd:complexType>
					                </xsd:element>
				              </xsd:sequence>
				              <xsd:sequence>
					                <xsd:element name="check" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>Details of the check in case of a check payment</xsd:documentation>
						                  </xsd:annotation>
						                  <xsd:complexType>
							                    <xsd:sequence>
								                      <xsd:element name="typeCode" type="xsd:token" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Code Type</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="accountHolder" type="xsd:normalizedString" minOccurs="0"
                                           maxOccurs="unbounded">
									                        <xsd:annotation>
										                          <xsd:documentation>Account Holder's name on the check </xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="bankName" type="xsd:normalizedString" minOccurs="0" maxOccurs="unbounded">
									                        <xsd:annotation>
										                          <xsd:documentation>Bank on wnich the check is drawn</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="checkNumber" type="xsd:token" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Check Number</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="IBAN" type="xsd:token" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>International bank account number. Used internationally to uniquely identify the account of a customer at a financial institution</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="routingNumber" type="xsd:token" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Routing Number of the bank on whom the check  is drawn</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="memo" type="xsd:normalizedString" minOccurs="0" maxOccurs="unbounded">
									                        <xsd:annotation>
										                          <xsd:documentation>Memo on the check</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="issuedDate" type="xsd:date" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>The Issued Date for the payment </xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
							                    </xsd:sequence>
						                  </xsd:complexType>
					                </xsd:element>
				              </xsd:sequence>
			            </xsd:choice>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="CreditCardPayment">
		          <xsd:sequence>
			            <xsd:element name="creditChargeAuthorization" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Credit Card Payment and authorization Details</xsd:documentation>
				              </xsd:annotation>
				              <xsd:complexType>
					                <xsd:complexContent>
						                  <xsd:extension base="ChargeAuthorization">
							                    <xsd:sequence>
								                      <xsd:element name="traceNumber" type="xsd:token" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Third party Payment (credit card system) system-provided trace number</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="voiceAuthorizationIndicator" type="xsd:boolean" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Indicates whether Authorization was done manually over the phone</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="creditCard" type="eo:CreditCard" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Details about the credit card to which an amount is requsted to be charged</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
							                    </xsd:sequence>
						                  </xsd:extension>
					                </xsd:complexContent>
				              </xsd:complexType>
			            </xsd:element>
			            <xsd:element name="creditChargeAuthorizationResponse" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Details about the authorization Response for an amount to be charged to a credit card</xsd:documentation>
				              </xsd:annotation>
				              <xsd:complexType>
					                <xsd:complexContent>
						                  <xsd:extension base="ChargeAuthorizationResponse">
							                    <xsd:sequence>
								                      <xsd:element name="AVSResponseCode" type="xsd:token" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Address Verification Service response code for swiped transactions where the zip code or address line information was provided as part of the request</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="securityCheckResponseCode" type="xsd:token" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Result of the CVV3/4 number verification</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
							                    </xsd:sequence>
						                  </xsd:extension>
					                </xsd:complexContent>
				              </xsd:complexType>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="DebitCardPayment">
		          <xsd:sequence>
			            <xsd:element name="debitChargeAuthorization" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Debit Card Payment and authorization Details</xsd:documentation>
				              </xsd:annotation>
				              <xsd:complexType>
					                <xsd:complexContent>
						                  <xsd:extension base="ChargeAuthorization">
							                    <xsd:sequence>
								                      <xsd:element name="debitCard" type="eo:DebitCard" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Details about the debit card to which an amount is requsted to be charged</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
							                    </xsd:sequence>
						                  </xsd:extension>
					                </xsd:complexContent>
				              </xsd:complexType>
			            </xsd:element>
			            <xsd:element name="debitChargeAuthorizationResponse" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Details about the authorization Response for an amount to be charged to a debit card</xsd:documentation>
				              </xsd:annotation>
				              <xsd:complexType>
					                <xsd:complexContent>
						                  <xsd:extension base="ChargeAuthorizationResponse">
							                    <xsd:sequence>
								                      <xsd:element name="traceNumber" type="xsd:token" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Trace Number</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
								                      <xsd:element name="debitNetworkCode" type="xsd:token" minOccurs="0">
									                        <xsd:annotation>
										                          <xsd:documentation>Code of the Debit network processing the debit charge</xsd:documentation>
									                        </xsd:annotation>
								                      </xsd:element>
							                    </xsd:sequence>
						                  </xsd:extension>
					                </xsd:complexContent>
				              </xsd:complexType>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="ChargeAuthorization">
		          <xsd:annotation>
			            <xsd:documentation>Charge Card Payment and authorization Details</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element name="transactionTypeCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Type of  card transaction attempted.  Values include: AUTHORIZATION, AUTHCAPTURE, VOICEAUTH, CAPTURE,  RETURN, VOID</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="chargeAmount" type="eo:Amount" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>In Payment transaction request currency</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="chargeTime" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Time of requesting the credit card authorization</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType name="ChargeAuthorizationResponse">
		          <xsd:annotation>
			            <xsd:documentation>Details about the authorization Response for an amount to be charged to a payment card</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element name="actionTypeCode" type="xsd:token" minOccurs="0"/>
			            <xsd:element name="responseCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Code that indicates the result of the authorization request</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="paymentSystemCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Payment system code returned during the initial authorization</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="authorizationTime" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Time at which the credit card payment request was authorized</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="authorizedAmount" type="eo:Amount" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>The Authorized  Charge Amount, in the  currency of the card issuer</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
		          </xsd:sequence>
	        </xsd:complexType>
         <xsd:complexType xmlns:ns1="http://esp.t-mobile.com/2012/eo" name="Attachment">
		          <xsd:sequence>
			            <xsd:element name="typeCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Type of attachment, e.g., URL, TEXT, Grapthics, Repository File, Folder</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="purposeCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Primary purpose of the attachment, e.g., Shipping, Manufacturing, Invoicing.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="name" type="xsd:normalizedString" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Name of the attachment</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="title" type="xsd:normalizedString" minOccurs="0" maxOccurs="unbounded">
				              <xsd:annotation>
					                <xsd:documentation>Title of the attachment. This is not the file name e.g. the title of a document is different from the file name of the document</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="sizeQuantity" type="eo:Quantity" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Size of the attachment</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="creationTime" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Date Time of creation of the document in the system of record</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:choice>
				              <xsd:sequence>
					                <xsd:element name="embeddedData" type="eo:BinaryObject" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>Element holding data embedded directly in the BOD instance</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
				              </xsd:sequence>
				              <xsd:sequence>
					                <xsd:element name="URI" type="xsd:anyURI" minOccurs="0">
						                  <xsd:annotation>
							                    <xsd:documentation>URI indicating where the attachment is located</xsd:documentation>
						                  </xsd:annotation>
					                </xsd:element>
				              </xsd:sequence>
			            </xsd:choice>
			            <xsd:element name="description" type="xsd:normalizedString" minOccurs="0"
                            maxOccurs="unbounded">
				              <xsd:annotation>
