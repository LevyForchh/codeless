         <xsd:complexType name="PaymentProfile">
		          <xsd:annotation>
			            <xsd:documentation>Payment encapsulates a Payment transaction between one payer account and one payee account. The deploying organization acts as creditor for the purpose of this object and hence this Payment is incoming to the Accounts Receivable system of the deploying company. One  Payment transaction may pay more than one Accounts Receivable Document in creditor's system.</xsd:documentation>
		          </xsd:annotation>
		          <xsd:sequence>
			            <xsd:element name="key" type="eo:ObjectKey" minOccurs="0" maxOccurs="unbounded"/>
			            <xsd:element name="paymentMethodCode" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Payment Type. Values can be "Cash", "Credit Card", "Direct Debit" etc.
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="creationTime" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Time when the Payment request was created
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="requestTime" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Time when the payer requested the Payment to post to the payer's account.
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="processingTime" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Time when the payee's Payment system processed the payment
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="authorizationTime" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Time when the payee's Payment system authorized the payment
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="confirmationNumber" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Payment confirmation number generated by the payee party's system.
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="requestAmount" type="eo:Amount" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Amount that the payer requests to pay.
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="authorizationAmount" type="eo:Amount" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Amount that the payment method authorizes.
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="description" type="xsd:normalizedString" minOccurs="0"
                            maxOccurs="unbounded"/>
			            <xsd:element name="paymentVendorId" type="xsd:token" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation>Third party that processed the payment on behalf of T-Mobile.</xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="paymentReceiptTime" type="xsd:dateTime" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
The date when the payment was received (ie. payment received by a third-party and will be entered into the system at a later date.
                                </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <xsd:element name="status" type="eo:Status" minOccurs="0">
				              <xsd:annotation>
					                <xsd:documentation> 
Status of the Payment. SAMPLE VALUES: Entered, Posted, Closed
                               </xsd:documentation>
				              </xsd:annotation>
			            </xsd:element>
			            <!--				<xsd:element name="currencyExchange" type="CurrencyExchange" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation> 
Currency exchange rate between the payment transaction request currency and the payment method currency
                               </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
-->
		</xsd:sequence>
		          <xsd:attribute name="id" type="eo:Identifier">
			            <xsd:annotation>
				              <xsd:documentation>Uniquely identifies a payment transaction.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
		          <xsd:attribute name="actionCode" type="xsd:token">
			            <xsd:annotation>
				              <xsd:documentation>Code directing the action to be taken with this payment record.</xsd:documentation>
			            </xsd:annotation>
		          </xsd:attribute>
	        </xsd:complexType>
         <xsd:complexType name="BillingAccountProfile">
		          <xsd:sequence>
			            <xsd:element name="accountNumber" type="xsd:token" minOccurs="0">
